{
    "name": "ubitcorp/laravel-cities",
    "description": "It provides continents, countries, cities (with div1 and div2) and districts.",
    "keywords": ["ubitcorp", "laravel-cities", "cities","country","continent","district", "hasan civelek"],
    "license": "MIT",
    "homepage": "https://github.com/ubitcorp/laravel-cities",
    "authors": [{
        "name": "Hasan Civelek",
        "email": "hasan@ubit.com.tr",
        "role": "Developer"
    }],
    "require": {
        "php": "~7.1",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x"
    },
    "autoload": {
        "psr-4": {
            "ubitcorp\\Cities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ubitcorp\\Cities\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ubitcorp\\Cities\\Providers\\CityServiceProvider"
            ],
            "aliases": {
                "Cities": "ubitcorp\\Cities\\Facade"
            }
        }
    }
}
